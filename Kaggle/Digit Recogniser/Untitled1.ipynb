    "import random\n",
    "\n",
    "def rollDie(number):\n",
    "    counts = [0] * 6\n",
    "    for i in range(number):\n",
    "        roll = random.randint(1,6)\n",
    "        counts[roll - 1] += 1\n",
    "    return counts"
   ]
  },
